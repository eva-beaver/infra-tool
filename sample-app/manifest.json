{ "version": "v1.00", 
  "type": "java", 
  "info": { 
    "id": 1, 
    "name": "sampleproject",
    "repository": "git@github.com:eva-beaver/infra-tool.git",
    "description": "Manifest Sample project.", 
    "categories": [],
    "environment": "Dev"
  },
  "build": {
    "buildType": "Maven",     
    "jdk": "8u131",
    "command": " clean install",
    "targetJarName": "sample-app-0.0.1-SNAPSHOT",
    "targetDir": "/target",
    "useWrapper": 0,
    "removeTarget": 0
  },
  "run": {
    "command": "java -jar  ",
    "targetJarName": "sample-app-0.0.1-SNAPSHOT",
    "entryPointClass": "sample-app-0.0.1-SNAPSHOT",
    "javaopts": "'-Xmx300m'"
  },
  "dockerFile": { 
    "generate": 1,
    "location": "",
    "name": ""
  },
  "docker": { 
    "baseImagePrefix": "",
    "repoName": "evabeaver",
    "buildImageName": "openjdk:8-jdk-alpine",
    "_baseImageName11": "adoptopenjdk/openjdk11",
    "baseImageName": "openjdk:8-jre-alpine",
    "containerName": "mycontainer",
    "version": "v2",
    "instanceName": "SampleProjectInstance",
    "dockerfileTemplate": "./DockerfileTemplate/Dockerfile",
    "mutilStageBuild": 1,
    "ports": "9010:9010,9011:9011",
    "memory": "",
    "swapMemory": "",
    "removeDocker": 1,
    "saveDockerRun": 1,
    "health": { 
      "cmd": "curl --silent --fail localhost:9010/actuator/health || exit 1",
      "interval": "5s",
      "retries": "12", 
      "timeout": "2s",     
      "startperiod": "60s"
    },
    "network": "admin-network",
    "ipMask": "171.1.1.0/24",
    "ipAddr": "171.1.1.7",
    "startNetwork": 1,
    "springBootProfiles": "docker",
    "environmentOptions": [
      {
        "name": "SPRING_BOOT_ADMIN_CLIENT_URL",
        "value": "http://171.18.0.2:9020"
      },
      {
        "name": "SPRING_APPLICATION_NAME",
        "value": "fred"
      }
    ],
    "useInDockerCompose": 1,
    "dockerFileFormatVersion": 2,
    "useProjectDockerFile": 0,
    "buildDockerImage": 1,
    "startDockerImage": 1,
    "pushDockerImage": 0
  },
  "environments": [ 
    { 
      "name": "Development",
      "memory": "512", 
      "instances": 1
    },
    { 
      "name": "Test",
      "memory": "1024", 
      "instances": 2
    },
    { 
      "name": "Stage",
      "memory": "2048", 
      "instances": 4
    },
    { 
      "name": "Production",
      "memory": "4096", 
      "instances": 6
    }
  ],  
  "dockerCompose": {
   "version": "3.7",
    "networkName": "dev-net",
    "networkType": "bridge",
    "ipMask": "171.1.1.0/24",
    "memory": "4096", 
    "processors": 4,
    "startAfterBuild": 1,
    "dockerNetworks": [
      {
        "networkName": "infra-net",
        "ipMask": "171.1.2.0/24"
      },
      {
        "networkName": "service-net",
        "ipMask": "171.1.3.0/24"
      }
    ]
  },
  "requires": [
